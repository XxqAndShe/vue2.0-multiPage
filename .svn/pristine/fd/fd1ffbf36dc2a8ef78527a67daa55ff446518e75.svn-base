define(["vue"],function(e){return e.extend({name:"layout-table",template:'<div class="layout-table">      <div class="layout-header">        <div class="layout-header-left" :class="{enter: headerVisible}" @mouseenter="headerMouseenter" @mouseleave="headerMouseleave">          <div class="header-left-hover-box">            <slot name="header-left-handler">left</slot>          </div>          <div class="filters-more">            <i style="color: #606266" class="el-icon-d-arrow-right"></i>          </div>        </div>        <div class="layout-header-right" :class="{enter: filtersVisible}" @mouseenter="filterMouseenter" @mouseleave="filterMouseleave">          <div class="layout-header-right-filters">            <div class="filters-hover-box">              <slot name="header-right-filters">filters</slot>            </div>            <div class="filters-more">              <i style="color: #606266" class="el-icon-d-arrow-right"></i>            </div>          </div>          <div class="layout-header-right-handler">            <slot name="header-right-handler">handler</slot>          </div>        </div>      </div>      <div class="layout-main">        <div class="main-settings">          <slot name="main-settings"></slot>        </div>        <div class="main-container">          <slot name="main">main</slot>        </div>      </div>      <div class="layout-footer">        <slot name="footer">footer</slot>      </div>    </div>',data:function(){return{headerVisible:!1,headerLeaveTimer:null,filtersVisible:!1,filtersLeaveTimer:null}},methods:{headerMouseenter:function(){clearInterval(this.headerLeaveTimer),this.headerVisible=!0},headerMouseleave:function(){var e=this;this.headerLeaveTimer=setTimeout(function(){e.headerVisible=!1},400)},filterMouseenter:function(){clearInterval(this.filtersLeaveTimer),this.filtersVisible=!0},filterMouseleave:function(){var e=this;this.filtersLeaveTimer=setTimeout(function(){e.filtersVisible=!1},400)}}})});